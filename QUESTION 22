#include<stdio.h>
int main()
{
  int n,m,c,d,first[10][10],second[10][10],sum[10][10],diff[10][10];
  printf("Enter the number of rows and columns of the firstmatrix");
  scanf("%d%d",&m,&n);
  printf("Enter the %d elements of the first matrix ", m*n);
    for(c = 0; c< m; c++)   
        for(d = 0; d < n; d++)   
            scanf("%d",&first[c][d]);
    for(c = 0; c < m; c++)   
        for(d = 0; d < n; d++)   
            scanf("%d", &second[c][d]);
    for(c = 0; c < m; c++)
        for(d = 0; d < n; d++)
            sum[c][d] = first[c][d] + second[c][d];
    for(c = 0; c < m; c++)
    {
        for(d = 0; d < n; d++)
        {
            printf("%d", sum[c][d]);
        }
        printf("\n");
    }
    for(c = 0; c < m; c++)
        for(d = 0; d < n; d++)
            diff[c][d] = first[c][d] - second[c][d];
    for(c = 0; c < m; c++)
    {
        for(d = 0; d < n; d++)
        {
            printf("%d", diff[c][d]);
        }
        printf("\n");
    }
    return 0;
}

