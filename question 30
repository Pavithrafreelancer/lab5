CODE
#include<stdio.h>
int main()
{
   int n,m,i,numbera=0,numberb=0;
   printf("Enter dimension of array1");
   scanf("%d",&n);
   int a[n];
   printf("Enter dimension of array2");
   scanf("%d",&m);
   int b[n];
   printf("Enter array1 a[%d]",n);
   for(i=0;i<n;i++)
   { scanf("%d",&a[i]);
     numbera=numbera*10+a[i];}
   printf("Enter array1 b[%d]",m);
   for(i=0;i<m;i++)
    {scanf("%d",&b[i]);
     numberb=numberb*10+b[i];}
    printf("Sum=%d",numbera+numberb);
   return 0;
}
OUTPUT
Enter dimension of array13
Enter dimension of array23
Enter array1 a[3]1
2
3
Enter array1 b[3]1
2
3
Sum=246
