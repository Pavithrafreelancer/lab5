#include <stdio.h>
int main() {
    int i, j, arr1[50][50],sum = 0,n,m= 0;
    printf("Input the size of the square matrix : ");
    scanf("%d", &n);
    m = n;
    for (i = 0; i < n; i++) {
        for (j = 0; j < n; j++) {
            printf("element - [%d],[%d] : ", i, j);
            scanf("%d", &arr1[i][j]);
        }
    }
    for (i = 0; i < n; i++) {
        m = m - 1; 
        for (j = 0; j < n; j++) {
            if (j == m) {
                sum = sum + arr1[i][j];
            }
        }
    }
    printf("Addition of the left Diagonal elements is : %d", sum);
    return 0;
}
